<Window x:Class="ADZZ.Rozliczenia___okno_i_strony.Rozliczenia"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ADZZ.Rozliczenia___okno_i_strony"
        mc:Ignorable="d"
        Title="Rozliczenia" Height="550" Width="950" MinHeight="550" MinWidth="950" MaxHeight="550" MaxWidth="950">
    <Grid>
        <Button x:Name="BtDodaj" Content="Dodaj" HorizontalAlignment="Left" Margin="45,45,0,0" VerticalAlignment="Top" Width="120" Click="BtDodaj_Click"/>
        <Button x:Name="BtEdytuj" Content="Edytuj" HorizontalAlignment="Left" Margin="45,80,0,0" VerticalAlignment="Top" Width="120" Click="BtEdytuj_Click"/>
        <Button x:Name="BtnWroc" Content="Wróć" HorizontalAlignment="Left" Margin="45,0,0,45" VerticalAlignment="Bottom" Width="120" Click="BtnWroc_Click"/>
        <Frame x:Name="RamkaFormularzaRozliczen" Margin="200,10,10,10" NavigationUIVisibility="Hidden" Navigated="RamkaFormularzaRozliczen_Navigated"/>
        <Button x:Name="BtZarzadzajCenami" Content="Zarzadzaj cenami" HorizontalAlignment="Left" Margin="45,117,0,0" VerticalAlignment="Top" Width="120" Click="BtZarzadzajCenami_Click"/>


    </Grid>
</Window>
