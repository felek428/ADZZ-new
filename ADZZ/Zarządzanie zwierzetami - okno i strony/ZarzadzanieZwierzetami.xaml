<Window x:Class="ADZZ.Zarządzanie_zwierzetami___okno_i_strony.ZarzadzanieZwierzetami"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ADZZ.Zarządzanie_zwierzetami___okno_i_strony"
        mc:Ignorable="d"
        Title="ZarzadzanieZwierzetami" Height="550" Width="950">
    <Grid>
        <Button x:Name="BtnDodaj" Content="Dodaj nowe zwierzęta" HorizontalAlignment="Left" Margin="45,45,0,0" VerticalAlignment="Top" Width="180"/>
        <Button x:Name="BtnEdytuj" Content="Edytuj istniejące zwierzęta" HorizontalAlignment="Left" Margin="45,80,0,0" VerticalAlignment="Top" Width="180"/>
        <Button x:Name="BtnUsun" Content="Usuń zwierzęta" HorizontalAlignment="Left" Margin="45,118,0,0" VerticalAlignment="Top" Width="180"/>
        <Button x:Name="BtnWroc" Content="Wróć" HorizontalAlignment="Left" Margin="45,0,0,45" VerticalAlignment="Bottom" Width="75" Height="22" Click="BtnWroc_Click"/>

    </Grid>
</Window>
